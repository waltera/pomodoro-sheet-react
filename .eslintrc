{
    "plugins": [
        "react",
        "react-redux"
    ],
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react-redux/recommended"
    ],
    "settings": {
      "react": {
        "pragma": "React",    // Pragma to use, default to "React"
        "version": "16.2.0",  // React version, default to the latest React stable release
        "flowVersion": "0.53" // Flow version
      }
    }
}
